{
    "compilerOptions": {
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
      "types": ["vitest/globals"]
    },
    "exclude": ["**/node_modules/**", "**/lib/**"],
  }